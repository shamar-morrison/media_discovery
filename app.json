{
  "expo": {
    "jsEngine": "hermes",
    "name": "Media Tracker",
    "slug": "media-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "media-discovery",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "youtube",
          "youtube"
        ]
      },
      "bundleIdentifier": "com.shamar2000.mediatracker"
    },
    "backgroundColor": "#1D1D1D",
    "androidNavigationBar": {
      "backgroundColor": "#1D1D1D"
    },
    "android": {
      "backgroundColor": "#1D1D1D",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#1D1D1D"
      },
      "package": "com.shamar2000.mediatracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1D1D1D",
          "dark": {
            "backgroundColor": "#1D1D1D"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/inter-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
